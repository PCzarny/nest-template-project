{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-template-project/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nest-template-project/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nest-template-project",
  "projects": {
    "nest-template-project": {
      "type": "application",
      "root": "apps/nest-template-project",
      "entryFile": "main",
      "sourceRoot": "apps/nest-template-project/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-template-project/tsconfig.app.json"
      }
    },
    "rabbit-consumer": {
      "type": "application",
      "root": "apps/rabbit-consumer",
      "entryFile": "main",
      "sourceRoot": "apps/rabbit-consumer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rabbit-consumer/tsconfig.app.json"
      }
    }
  }
}